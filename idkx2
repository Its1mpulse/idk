then local a=math.acos(_) local _=1/math.sin(a) b=math.sin((1-e)*a)*_ c=math.sin(e*a)*_ else b=1-e c=e end else if(1+_)>0.0001 then local a=math.acos(-_) local _=1/math.sin(a) b=math.sin((e-1)*a)*_ c=math.sin(e*a)*_ else b=e-1 c=e end end return a[1]*b+d[1]*c,a[2]*b+d[2]*c,a[3]*b+d[3]*c,a[4]*b+d[4]*c end function Clerp(_,a,i)local f={QFCF(_)} local g={QFCF(a)} local d,e,c=_.x,_.y,_.z local b,a,_=a.x,a.y,a.z local h=1-i return QTCF(h*d+i*b,h*e+i*a,h*c+i*_,QS(f,g,i))end local Y={} function Y.nub(_,b,a)return _:Lerp(b,a)end function Y.Clerp(b,a,_)return b:Lerp(a,_)end Qlerp=function(b,a,_)return b:Lerp(a,math.clamp(_,0,1))end ArtificialHB=Instance.new("\x42\x69\x6E\x64\x61\x62\x6C\x65\x45\x76\x65\x6E\x74",script) ArtificialHB.Name="\x48\x65\x61\x72\x74\x62\x65\x61\x74" script:WaitForChild("\x48\x65\x61\x72\x74\x62\x65\x61\x74") frame=1/60 tf=0 allowframeloss=false tossremainder=false lastframe=tick() script.Heartbeat:Fire() game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").Heartbeat:Connect(function(a,_)tf=tf+a if tf>=frame then if allowframeloss then script.Heartbeat:Fire() lastframe=tick()else for _=1,math.floor(tf/frame)do script.Heartbeat:Fire()end lastframe=tick()end if tossremainder then tf=0 else tf=tf-frame*math.floor(tf/frame)end end end) function swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end function Swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end SWAIT=Swait local _=Instance.new("\x53\x6F\x75\x6E\x64") local N=game.Players.LocalPlayer.Character local I=I(I(I(N,"\x53\x74\x61\x72\x73\x6C\x61\x79\x65\x72\x20\x52\x61\x69\x6C\x67\x75\x6E","\x41\x63\x63\x65\x73\x73\x6F\x72\x79"),"\x48\x61\x6E\x64\x6C\x65","\x42\x61\x73\x65\x50\x61\x72\x74"),"\x61\x74\x74\x31\x5F\x48\x61\x6E\x64\x6C\x65","\x41\x74\x74\x61\x63\x68\x6D\x65\x6E\x74") I.Parent=N["\x52\x69\x67\x68\x74\x20\x41\x72\x6D"] I.Position=Vector3.new(0,-1.5,-0.7) I.Orientation=Vector3.new(-0,90,-45) local _=1 function AttackGyro()local _=Instance.new("\x42\x6F\x64\x79\x47\x79\x72\x6F",R) _.D=25 _.P=20000 _.MaxTorque=Vector3.new(0,4000000,0) _.CFrame=CFrame.new(R.Position,g.Hit.p) coroutine.resume(coroutine.create(function()repeat if K then wait(math.huge)end swait() _.CFrame=CFrame.new(R.Position,g.Hit.p)until P==false _:Destroy()end))end coroutine.resume(coroutine.create(function()while not K do for _=0,1,(1/60)do swait() if not k then v.NeonParts.Color=Color3.fromHSV(_,1,1) if X.Value=="\x69\x4E\x53\x61\x4E\x69\x54\x59"then v.NeonParts.Color=Color3.fromHSV(math.random(0,19)/20,1,1)elseif X.Value=="\x45\x72\x72\x30\x72"then v.NeonParts.Color=Color3.fromRGB(0,math.clamp(S.PlaybackLoudness-255,0,255),0)elseif X.Value=="\x47\x6C\x69\x74\x63\x68"then v.NeonParts.Color=Color3.fromHSV(_,1,math.clamp((S.PlaybackLoudness/255)-.5,0,1))elseif X.Value=="\x41\x6C\x61\x73\x74\x6F\x72"then v.NeonParts.Color=Color3.new(S.PlaybackLoudness/500,0,0)elseif X.Value=="\x6B\x41\x72\x6D\x41"then v.NeonParts.Color=Color3.new(1,1,1)elseif X.Value=="\x70\x65\x74\x66\x72\x6F\x67\x65"then v.NeonParts.Color=BrickColor.Random().Color end end end end end)) function attackone()P=true n=16 AttackGyro() for _=0,.5,.05 do swait() ja.C0=Clerp(ja.C0,da*CFrame.new(0+.5*math.cos(na/50),0,0-.5*math.sin(na/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) ma.C0=Clerp(ma.C0,u*CFrame.new()*CFrame.Angles(math.rad(15),math.rad(0),math.rad(-30)),1/3) ha.C0=Clerp(ha.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(90),math.rad(0),math.rad(30))*w,1/3) ia.C0=Clerp(ia.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(30),math.rad(0),math.rad(0))*F,1/3) ka.C0=Clerp(ka.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(na/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(na/37)),math.rad(0),math.rad(0)),.7/3) la.C0=Clerp(la.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(na/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(na/41)),math.rad(0),math.rad(0)),.7/3)end for _=0,.5,.075 do swait() ja.C0=Clerp(ja.C0,da*CFrame.new(0+.5*math.cos(na/50),0,0-.5*math.sin(na/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) ma.C0=Clerp(ma.C0,u*CFrame.new()*CFrame.Angles(math.rad(10),math.rad(0),math.rad(-60)),1/3) ha.C0=Clerp(ha.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(160),math.rad(-20),math.rad(60))*w,1/3) ia.C0=Clerp(ia.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(40),math.rad(5),math.rad(5))*F,1/3) ka.C0=Clerp(ka.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(na/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(na/37)),math.rad(0),math.rad(0)),.7/3) la.C0=Clerp(la.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(na/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(na/41)),math.rad(0),math.rad(0)),.7/3)end n=50 P=false end local d=false iyflyspeed=1 function sFLY()if not j then d=false else local c={F=0,B=0,L=0,R=0,Q=0,E=0} local b={F=0,B=0,L=0,R=0,Q=0,E=0} local a=0 local function _()d=true l=Instance.new('\x42\x6F\x64\x79\x47\x79\x72\x6F',R) o=Instance.new('\x42\x6F\x64\x79\x56\x65\x6C\x6F\x63\x69\x74\x79',R) l.MaxTorque=Vector3.new(math.huge,math.huge,math.huge) l.P=1e4 l.CFrame=R.CFrame o.Velocity=Vector3.new(0,0,0) o.MaxForce=Vector3.new(math.huge,math.huge,math.huge) coroutine.resume(coroutine.create(function()repeat if K then wait(math.huge)end game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").RenderStepped:Wait() if not k then if c.L+c.R~=0 or c.F+c.B~=0 or c.Q+c.E~=0 then a=50 elseif not(c.L+c.R~=0 or c.F+c.B~=0 or c.Q+c.E~=0)and a~=0 then a=0 end if(c.L+c.R)~=0 or(c.F+c.B)~=0 or(c.Q+c.E)~=0 then o.Velocity=((workspace.CurrentCamera.CoordinateFrame.lookVector*(c.F+c.B))+((workspace.CurrentCamera.CoordinateFrame*CFrame.new(c.L+c.R,(c.F+c.B+c.Q+c.E)*0.2,0).Position)-workspace.CurrentCamera.CoordinateFrame.p))*a b={F=c.F,B=c.B,L=c.L,R=c.R}elseif(c.L+c.R)==0 and(c.F+c.B)==0 and(c.Q+c.E)==0 and a~=0 then o.Velocity=((workspace.CurrentCamera.CoordinateFrame.lookVector*(b.F+b.B))+((workspace.CurrentCamera.CoordinateFrame*CFrame.new(b.L+b.R,(b.F+b.B+c.Q+c.E)*0.2,0).Position)-workspace.CurrentCamera.CoordinateFrame.p))*a else o.Velocity=Vector3.new(0,0,0)end l.CFrame=workspace.CurrentCamera.CoordinateFrame end until not d c={F=0,B=0,L=0,R=0,Q=0,E=0} b={F=0,B=0,L=0,R=0,Q=0,E=0} a=0 if not k then for _,_ in pairs(R:GetChildren())do if _:IsA("\x42\x6F\x64\x79\x4D\x6F\x76\x65\x72")then _:Destroy()end end R.CFrame=CFrame.new(R.Position)end end))end local a=e:GetMouse() a.KeyDown:connect(function(_)if K then wait(math.huge)end if _:lower()=='\x77'then c.F=iyflyspeed elseif _:lower()=='\x73'then c.B=-iyflyspeed elseif _:lower()=='\x61'then c.L=-iyflyspeed elseif _:lower()=='\x64'then c.R=iyflyspeed elseif _:lower()=='\x65'then c.Q=iyflyspeed*2 elseif _:lower()=='\x71'then c.E=-iyflyspeed*2 end end) a.KeyUp:connect(function(_)if K then wait(math.huge)end if _:lower()=='\x77'then c.F=0 elseif _:lower()=='\x73'then c.B=0 elseif _:lower()=='\x61'then c.L=0 elseif _:lower()=='\x64'then c.R=0 elseif _:lower()=='\x65'then c.Q=0 elseif _:lower()=='\x71'then c.E=0 end end) _()end end g.Button1Down:Connect(function()if K then wait(math.huge)end if not P and(X.Value~="\x46\x61\x76\x65"and X.Value~="\x53\x6D\x6F\x67"and X.Value~="\x63\x6F\x72\x72\x75\x70\x74"and X.Value~="\x4D\x61\x64\x64\x6F\x78\x4C\x43\x76\x33"and X.Value~="\x75\x77\x75\x6D\x61\x6E"and X.Value~="\x73\x6E\x61\x6B\x65\x73\x6B\x69\x64"and X.Value~="\x52\x65\x67\x65\x6C\x65"and X.Value~="\x55\x6E\x63\x6F\x6C\x6F\x72\x65\x64"and X.Value~="\x4D\x65\x6D\x65\x72"and X.Value~="\x53\x69\x74"and X.Value~="\x4B\x72\x75\x6D\x70"and X.Value~="\x4F\x4F\x4F\x4F\x4F"and X.Value~="\x6E\x75\x6C\x6C\x65\x64"and X.Value~="\x75\x77\x75")then attackone()end end) g.KeyDown:Connect(function(_)if K then wait(math.huge)end _=_:lower() if _=="\x6D"then if ba.Value~="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x33\x32\x34\x37\x3
)end r.Transparency=r.Transparency-a/u if x=="\x42\x6C\x6F\x63\x6B"then r.CFrame=q*CFrame.Angles(math.rad(math.random(0,360)),math.rad(math.random(0,360)),math.rad(math.random(0,360)))else r.CFrame=r.CFrame*CFrame.Angles(math.rad(e),math.rad(i),math.rad(f))end if t~=nil then local _=r.Orientation r.CFrame=CFrame.new(r.Position,t)*CFrame.new(0,0,-g) r.Orientation=_ end end end r.Transparency=1 if o==false then r:Destroy()else repeat if K then wait(math.huge)end swait()until r:FindFirstChildOfClass("\x53\x6F\x75\x6E\x64")==nil r:Destroy()end else if o==false then r:Destroy()else repeat if K then wait(math.huge)end swait()until r:FindFirstChildOfClass("\x53\x6F\x75\x6E\x64")==nil r:Destroy()end end end))end function Lightning(a,j,_,h,_,b,i,g,d,c,f,e)local k=(a-j).Magnitude local m=math.floor(math.clamp(k/10,1,20)) local l=a local h={-h,h} for a=1,m do local n=Instance.new("\x50\x61\x72\x74",w) n.Name=t() n.TopSurface=0 n.Material="\x4E\x65\x6F\x6E" n.BottomSurface=0 n.Anchored=true n.Locked=true n.Transparency=0 n.Color=_ n.formFactor="\x43\x75\x73\x74\x6F\x6D" n.CanCollide=false n.Size=Vector3.new(0.1,0.1,k/m) local _=Vector3.new(h[math.random(1,2)],h[math.random(1,2)],h[math.random(1,2)]) local _=CFrame.new(l,j)*CFrame.new(0,0,k/m).Position+_ if m==a then local _=(l-j).Magnitude n.Size=Vector3.new(0.1,0.1,_) n.CFrame=CFrame.new(l,j)*CFrame.new(0,0,-_/2)else n.CFrame=CFrame.new(l,_)*CFrame.new(0,0,k/m/2)end l=n.CFrame*CFrame.new(0,0,k/m/2).Position n:Destroy() Effect({Time=b,EffectType="\x42\x6F\x78",Size=Vector3.new(i,i,n.Size.Z),Size2=Vector3.new(g,g,n.Size.Z),Transparency=d,Transparency2=e or 1,CFrame=n.CFrame,MoveToPos=nil,RotationX=nil,RotationY=nil,RotationZ=nil,Material="\x4E\x65\x6F\x6E",Color=n.Color,SoundID=nil,SoundPitch=nil,SoundVolume=nil,UseBoomerangMath=c,Boomerang=0,SizeBoomerang=f})end end function FireArc(d,a,e,c,_)if _==false then local b=CFrame.new(d.Position,a) local a=(d.Position-a).Magnitude for _=1,e do swait() d.CFrame=b*CFrame.new(0,(e/200)+((e/c)-((_*2)/c)),-a/e) b=d.CFrame end elseif _==true then coroutine.resume(coroutine.create(function()local b=CFrame.new(d.Position,a) local a=(d.Position-a).Magnitude for _=1,e do swait() d.CFrame=b*CFrame.new(0,(e/200)+((e/c)-((_*2)/c)),-a/e) b=d.CFrame end end))end end function MakeForm(_,a)if a=="\x43\x79\x6C"then local _=Instance.new("\x43\x79\x6C\x69\x6E\x64\x65\x72\x4D\x65\x73\x68",_)elseif a=="\x42\x61\x6C\x6C"then local _=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",_) _.MeshType="\x53\x70\x68\x65\x72\x65"elseif a=="\x57\x65\x64\x67\x65"then local _=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",_) _.MeshType="\x57\x65\x64\x67\x65"end end function AttackGyro()local _=Instance.new("\x42\x6F\x64\x79\x47\x79\x72\x6F",P) _.D=25 _.P=20000 _.MaxTorque=Vector3.new(0,4000000,0) _.CFrame=CFrame.new(P.Position,g.Hit.p) coroutine.resume(coroutine.create(function()repeat if K then wait(math.huge)end swait() _.CFrame=CFrame.new(P.Position,g.Hit.p)until M==false _:Destroy()end))end coroutine.resume(coroutine.create(function()while not K do for _=0,1,(1/60)do swait() if not o then fa.NeonParts.Color=Color3.fromHSV(_,1,1) if T.Value=="\x69\x4E\x53\x61\x4E\x69\x54\x59"then fa.NeonParts.Color=Color3.fromHSV(math.random(0,19)/20,1,1)elseif T.Value=="\x45\x72\x72\x30\x72"then fa.NeonParts.Color=Color3.fromRGB(0,math.clamp(Q.PlaybackLoudness-255,0,255),0)elseif T.Value=="\x46\x41\x53\x54\x20\x42\x4F\x49\x49\x49"then fa.NeonParts.Color=Color3.fromRGB(0,0,math.clamp(Q.PlaybackLoudness-191,0,255))elseif T.Value=="\x47\x6C\x69\x74\x63\x68"then fa.NeonParts.Color=Color3.fromHSV(_,1,math.clamp((Q.PlaybackLoudness/255)-.5,0,1))elseif T.Value=="\x41\x6C\x61\x73\x74\x6F\x72"then fa.NeonParts.Color=Color3.new(Q.PlaybackLoudness/500,0,0)elseif T.Value=="\x6B\x41\x72\x6D\x41"then fa.NeonParts.Color=Color3.new(1,1,1)elseif T.Value=="\x70\x65\x74\x66\x72\x6F\x67\x65"then fa.NeonParts.Color=BrickColor.Random().Color end end end end end)) function attackone()M=true m=16 AttackGyro() for _=0,.5,.05 do swait() ia.C0=Clerp(ia.C0,ba*CFrame.new(0+.5*math.cos(ma/50),0,0-.5*math.sin(ma/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) la.C0=Clerp(la.C0,x*CFrame.new()*CFrame.Angles(math.rad(15),math.rad(0),math.rad(-30)),1/3) ga.C0=Clerp(ga.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(90),math.rad(0),math.rad(30))*z,1/3) ha.C0=Clerp(ha.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(30),math.rad(0),math.rad(0))*D,1/3) ja.C0=Clerp(ja.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(ma/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(ma/37)),math.rad(0),math.rad(0)),.7/3) ka.C0=Clerp(ka.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(ma/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(ma/41)),math.rad(0),math.rad(0)),.7/3) ca.C0=Clerp(ca.C0,CFrame.new(.05,-1,-.15)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(0)),1/2)end for _=0,.5,.075 do swait() ia.C0=Clerp(ia.C0,ba*CFrame.new(0+.5*math.cos(ma/50),0,0-.5*math.sin(ma/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) la.C0=Clerp(la.C0,x*CFrame.new()*CFrame.Angles(math.rad(10),math.rad(0),math.rad(-60)),1/3) ga.C0=Clerp(ga.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(160),math.rad(-20),math.rad(60))*z,1/3) ha.C0=Clerp(ha.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(40),math.rad(5),math.rad(5))*D,1/3) ja.C0=Clerp(ja.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(ma/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(ma/37)),math.rad(0),math.rad(0)),.7/3) ka.C0=Clerp(ka.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(ma/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(ma/41)),math.rad(0),math.rad(0)),.7/3) ca.C0=Clerp(ca.C0,CFrame.new(.05,-1,-.15)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(0)),1/2)end m=50 M=false end local e=false iyflyspeed=1 function sFLY()if not h then e=false else local d={F=0,B=0,L=0,R=0,Q=0,E=0} local b={F=0,B=0,L=0,R=0,Q=0,E=0} local a=0 local function _()e=true k=Instance.new('\x42\x6F\x64\x79\x47\x79\x72\x6F',P) n=Instance.new('\x42\x6F\x64\x79\x56\x65\x6C\x6F\x63\x69\x74\x79',P) k.MaxTorque=Vector3.new(math.huge,math.huge,math.huge) k.P=1e4 k.CFrame=P.CFrame n.Velocity=Vector3.new(0,0,0) n.MaxForce=Vector3.new(math.huge,math.huge,math.huge) coroutine.resume(coroutine.create(function()repeat if K then wait(math.huge)end game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").RenderStepped:Wait() if not o then if d.L+d.R~=0 or d.F+d.B~=0 or d.Q+d.E~=0 then a=50 elseif not(d.L+d.R~=0 or d.F+d.B~=0 or d.Q+d.E~=0)and a~=0 then a=0 end if(d.L+d.R)~=0 or(d.F+d.B)~=0 or(d.Q+d.E)~=0 then n.Velocity=((workspace.CurrentCamera.CoordinateFrame.lookVector*(d.F+d.B))+((workspace.CurrentCamera.CoordinateFrame*CFrame.new(d.L+d.R,(d.F+d.B+d.Q+d.E)*0.2,0).Position)-workspace.CurrentCamera.CoordinateFrame.p))*a b={F=d.F,B=d.B,L=d.L,R=d.R}elseif(d.L+d.R)==0 and(d.F+d.B)==0 and(d.Q+d.E)==0 and a~=0 then n.Velocity=((workspace.CurrentCamera.CoordinateFrame.lookVector*(b.F+b.B))+((workspace.CurrentCamera.CoordinateFrame*CFrame.new(b.L+b.R,(b.F+b.B+d.Q+d.E)*0.2,0).Position)-workspace.CurrentCamera.CoordinateFrame.p))*a else n.Velocity=Vector3.new(0,0,0)end k.CFrame=workspace.CurrentCamera.CoordinateFrame end until not e d={F=0,B=0,L=0,R=0,Q=0,E=0} b={F=0,B=0,L=0,R=0,Q=0,E=0} a=0 if not o then for _,_ in pairs(P:GetChildren())do if _:IsA("\x42\x6F\x64\x79\x4D\x6F\x76\x65\x72")then _:Destroy()end end P.CFrame=CFrame.new(P.Position)end end))end local a=c:GetMouse() a.KeyDown:connect(function(_)if K then wait(math.huge)end if _:lower()=='\x77'then d.F=iyflyspeed elseif _:lower()=='\x73'then d.B=-iyflyspeed elseif _:lower()=='\x61'then d.L=-iyflyspeed elseif _:lower()=='\x64'then d.R=iyflyspeed elseif _:lower()=='\x65'then d.Q=iyflyspeed*2 elseif _:lower()=='\x71'then d.E=-iyflyspeed*2 end end) a.KeyUp:connect(function(_)if K then wait(math.huge)end if _:lower()=='\x77'then d.F=0 elseif _:lower()=='\x73'then d.B=0 elseif _:lower()=='\x61'then d.L=0 elseif _:lower()=='\x64'then d.R=0 elseif _:lower()=='\x65'then d.Q=0 elseif _:lower()=='\x71'then d.E=0 end end) _()end end function sphere(e,c,b,_,d,a)local c=c local f=Instance.new("\x50\x61\x72\x74",w) f.Anchored=true f.BrickColor=a f.CanCollide=false f.FormFactor=3 f.Name="\x52\x69\x6E\x67" f.Material="\x4E\x65\x6F\x6E" f.Size=Vector3.new(1,1,1) f.Transparency=0 f.TopSurface=0 f.BottomSurface=0 f.CFrame=b local a=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",f) a.MeshType="\x53\x70\x68\x65\x72\x65" a.Scale=_ local b=1 if c=="\x41\x64\x64"then b=1*d elseif c=="\x44\x69\x76\x69\x64\x65"then b=1/d end coroutine.resume(coroutine.create(function()for _=0,10/e,0.1 do swait() if c=="\x41\x64\x64"then b=b-0.01*d/e elseif c=="\x44\x69\x76\x69\x64\x65"then b=b-0.01/d*e end f.BrickColor=BrickColor.random() f.Transparency=f.Transparency+0.01*e a.Scale=a.Scale+Vector3.new(b*e,b*e,b*e)end f:Destroy()end))end function sphere2(i,c,b,a,h,e,d,_)local c=c local j=Instance.new("\x50\x61\x72\x74",w) j.Anchored=true j.BrickColor=_ j.CanCollide=false j.FormFactor=3 j.Name="\x52\x69\x6E\x67" j.Material="\x4E\x65\x6F\x6E" j.Size=Vector3.new(1,1,1) j.Transparency=0 j.TopSurface=0 j.BottomSurface=0 j.CFrame=b local b=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",j) b.MeshType="\x53\x70\x68\x65\x72\x65" b.Scale=a local a=1 local f=1 local g=1 if c=="\x41\x64\x64"then a=1*h f=1*e g=1*d elseif c=="\x44\x69\x76\x69\x64\x65"then a=1/h f=1/e g=1/d end coroutine.resume(coroutine.create(function()for _=0,10/i,0.1 do swait() if c=="\x41\x64\x64"then a=a-0.01*h/i f=f-0.01*h/i g=g-0.01*h/i elseif c=="\x44\x69\x76\x69\x64\x65"then a=a-0.01/h*i f=f-0.01/h*i g=g-0.01/h*i end j.Transparency=j.Transparency+0.01*i b.Scale=b.Scale+Vector3.new(a*i,f*i,g*i)end j:Destroy()end))end function PixelBlockX(j,g,_,a,c,b,e,i,d,f)local h=_ local k=Instance.new("\x50\x61\x72\x74",w) k.Anchored=true k.BrickColor=d k.CanCollide=false k.FormFactor=3 k.Name="\x52\x69\x6E\x67" k.Material="\x4E\x65\x6F\x6E" k.Size=Vector3.new(1,1,1) k.Transparency=0 k.TopSurface=0 k.BottomSurface=0 k.CFrame=a k.CFrame=k.CFrame+k.CFrame.lookVector*f local d=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",k) d.MeshType="\x42\x72\x69\x63\x6B" d.Scale=Vector3.new(c,b,e) local b=1 local a=g/10 if h=="\x41\x64\x64"then b=1*i elseif h=="\x44\x69\x76\x69\x64\x65"then b=1/i end coroutine.resume(coroutine.create(function()for _=0,10/j,0.1 do swait() if h=="\x41\x64\x64"then b=b-0.01*i/j elseif h=="\x44\x69\x76\x69\x64\x65"then b=b-0.01/i*j end k.BrickColor=BrickColor.random() a=a-0.01*g*j/10 k.CFrame=k.CFrame+k.CFrame.lookVector*a*j k.Transparency=k.Transparency+0.01*j d.Scale=d.Scale-Vector3.new(b*j,b*j,b*j)end k:Destroy()end))end function sphereMK(j,g,_,b,a,e,d,h,c,f)local i=_ local k=Instance.new("\x50\x61\x72\x74",w) k.Anchored=true k.BrickColor=c k.CanCollide=false k.FormFactor=3 k.Name="\x52\x69\x6E\x67" k.Material="\x4E\x65\x6F\x6E" k.Size=Vector3.new(1,1,1) k.Transparency=0 k.TopSurface=0 k.BottomSurface=0 k.CFrame=b k.CFrame=k.CFrame+k.CFrame.lookVector*f local b=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",k) b.MeshType="\x53\x70\x68\x65\x72\x65" b.Scale=Vector3.new(a,e,d) local c=1 local a=g if i=="\x41\x64\x64"then c=1*h elseif i=="\x44\x69\x76\x69\x64\x65"then c=1/h end coroutine.resume(coroutine.create(function()for _=0,10/j,0.1 do swait() if i=="\x41\x64\x64"then c=c-0.01*h/j elseif i=="\x44\x69\x76\x69\x64\x65"then c=c-0.01/h*j end k.BrickColor=BrickColor.random() a=a-0.01*g*j k.CFrame=k.CFrame+k.CFrame.lookVector*a*j k.Transparency=k.Transparency+0.01*j b.Scale=b.Scale+Vector3.new(c*j,c*j,0)end k:Destroy()end))end function slash(j,f,b,e,a,g,d,c,h,_)local i=a local a=b local k=Instance.new("\x50\x61\x72\x74",w) k.Anchored=true k.BrickColor=_ k.CanCollide=false k.FormFactor=3 k.Name="\x52\x69\x6E\x67" k.Material="\x4E\x65\x6F\x6E" k.Size=Vector3.new(1,1,1) k.Transparency=0 if g=="\x49\x6E"then k.Transparency=1 end k.TopSurface=0 k.BottomSurface=0 k.CFrame=d local d=Instance.new("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",k) d.MeshType="\x46\x69\x6C\x65\x4D\x65\x73\x68" if e=="\x4E\x6F\x72\x6D\x61\x6C"then d.MeshId="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x36\x36\x32\x35\x38\x36\x38\x35\x38"elseif e=="\x52\x6F\x75\x6E\x64"then d.MeshId="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x36\x36\x32\x35\x38\x35\x30\x35\x38"end d.Scale=c local c=1/10 if i=="\x41\x64\x64"then c=1*h/10 elseif i=="\x44\x69\x76\x69\x64\x65"then c=1/h/10 end local b=math.random(1,2) coroutine.resume(coroutine.create(function()for _=0,10/j,0.1 do swait() if i=="\x41\x64\x64"then c=c-0.01*h/j/10 elseif i=="\x44\x69\x76\x69\x64\x65"then c=c-0.01/h*j/10 end if a==true then if b==1 then k.CFrame=k.CFrame*CFrame.Angles(0,math.rad(f*j/2),0)elseif b==2 then k.CFrame=k.CFrame*CFrame.Angles(0,math.rad(-f*j/2),0)end end if g=="\x4F\x75\x74"then k.Transparency=k.Transparency+0.01*j elseif g=="\x49\x6E"then k.Transparency=k.Transparency-0.01*j end d.Scale=d.Scale+Vector3.new(c*j/10,0,c*j/10)end k:Destroy()end))end local _=T.Value g.Button1Down:Connect(function()if K then wait(math.huge)end if not M and(T.Value~="\x46\x61\x76\x65"and _~="\x4D\x65\x6D\x65\x72"and _~="\x53\x69\x74"and T.Value~="\x4B\x72\x75\x6D\x70"and T.Value~="\x4F\x4F\x4F\x4F\x4F"and T.Value~="\x6E\x75\x6C\x6C\x65\x64"and T.Value~="\x75\x77\x75"and T.Value~="\x68\x65\x6C\x6C")then attackone()end end) g.KeyDown:Connect(function(_)if K then wait(math.huge)end _=_:lower() if _=="\x6D"then if Z.Value~="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x33\x32\x34\x37\x34\x30\x34\x39\x35\x35"and Z.Value~="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x32\x37\x38\x35\x30\x31\x33\x30\x30"and Z.Value~="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x34\x39\x32\x31\x36\x34\x37\x36\x36\x30"and Z.Value~="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F\x35\x30\x31\x35\x34\x32\x31\
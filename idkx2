tance.new(_) c.Parent=a if(b)then for _,a in next,b do pcall(function()c[_]=a end)end end return c end function chatfunc(_,a)local b=string.gsub(_,"\xE2\x80\x8B","") local _=coroutine.wrap(function()local _=x:FindFirstChild("\x54\x61\x6C\x6B\x69\x6E\x67\x42\x69\x6C\x6C\x42\x6F\x61\x72\x64") if _ then _:Destroy()end local _=Instance.new("\x42\x69\x6C\x6C\x62\x6F\x61\x72\x64\x47\x75\x69",x) _.Size=UDim2.new(0,9999,2,0) _.StudsOffset=Vector3.new(0,5,0) _.Adornee=x _.Name="\x54\x61\x6C\x6B\x69\x6E\x67\x42\x69\x6C\x6C\x42\x6F\x61\x72\x64" local e=Instance.new("\x54\x65\x78\x74\x4C\x61\x62\x65\x6C",_) e.BackgroundTransparency=1 e.BorderSizePixel=0 e.Text="" e.Font="\x42\x61\x6E\x67\x65\x72\x73" e.TextScaled=true e.TextStrokeTransparency=0 coroutine.resume(coroutine.create(function()while not K do local b,_,a=Color3:ToHSV(v.NeonParts.Color) e.TextColor3=Color3.fromHSV(b,_,a/2) swait()end end)) e.TextStrokeColor3=d e.Size=UDim2.new(1,0,1,0) local c=Instance.new("\x54\x65\x78\x74\x4C\x61\x62\x65\x6C",_) c.BackgroundTransparency=1 c.BorderSizePixel=0 c.Text="" c.Font="\x42\x61\x6E\x67\x65\x72\x73" c.TextScaled=true c.TextStrokeTransparency=0 coroutine.resume(coroutine.create(function()while not K do c.TextColor3=v.NeonParts.Color swait()end end)) c.TextStrokeColor3=d c.Size=UDim2.new(1,0,1,0) coroutine.resume(coroutine.create(function()while not K and _~=nil do swait() if a then local _={"\x43\x6F\x64\x65","\x41\x72\x69\x61\x6C","\x43\x61\x72\x74\x6F\x6F\x6E","\x47\x6F\x74\x68\x61\x6D","\x48\x69\x67\x68\x77\x61\x79","\x4A\x6F\x73\x65\x66\x69\x6E\x53\x61\x6E\x73","\x4C\x65\x67\x61\x63\x79","\x4E\x75\x6E\x69\x74\x6F","\x4D\x65\x72\x72\x69\x77\x65\x61\x74\x68\x65\x72","\x52\x6F\x62\x6F\x74\x6F","\x53\x63\x69\x46\x69","\x53\x6F\x75\x72\x63\x65\x53\x61\x6E\x73","\x55\x62\x75\x6E\x74\x75"} local _=_[math.random(1,#_)] e.Font=_ c.Font=_ end e.Position=UDim2.new(0,math.random(-3,3),0,math.random(-3,3)) c.Position=UDim2.new(0,math.random(-3,3),0,math.random(-3,3))end end)) for _=1,str
r:remove()end else if n==false then r:remove()else repeat if K then wait(HUGE)end swait()until m.Playing==false r:remove()end end end))end local H=1 function Effect(o)local x=(o.EffectType or"\x53\x70\x68\x65\x72\x65") local v=(o.Size or Vector3.new(1,1,1)) local j=(o.Size2 or Vector3.new()) local k=(o.Transparency or 0) local d=(o.Transparency2 or 1) local q=(o.CFrame or B.CFrame) local t=(o.MoveToPos or nil) local e=(o.RotationX or 0) local i=(o.RotationY or 0) local f=(o.RotationZ or 0) local c=(o.Material or"\x4E\x65\x6F\x6E") local g=(o.Color or r) local _,a,_=Color3:ToHSV(g) local h=false if a>.1 then h=true end local u=(o.Time or 45) local l=(o.SoundID or nil) local n=(o.SoundPitch or nil) local m=(o.SoundVolume or nil) local p=(o.UseBoomerangMath or false) local b=(o.Boomerang or 0) local a=(o.SizeBoomerang or 0) coroutine.resume(coroutine.create(function()local o=false local _=nil local r=CreatePart(3,w,c,0,k,r,"\x45\x66\x66\x65\x63\x74",Vector3.new(1,1,1),true) if h then coroutine.resume(coroutine.create(function()while not K do r.Color=fa.NeonParts.Color swait()end end))end if l~=nil and n~=nil and m~=nil then o=true _=CreateSound(l,r,m,n,false)end r.Color=g local l=nil if x=="\x53\x70\x68\x65\x72\x65"then l=CreateMesh("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",r,"\x53\x70\x68\x65\x72\x65","","",v,Vector3.new())elseif x=="\x42\x6C\x6F\x63\x6B"or x=="\x42\x6F\x78"then l=Instance.new("\x42\x6C\x6F\x63\x6B\x4D\x65\x73\x68",r) l.Scale=v elseif x=="\x57\x61\x76\x65"then l=CreateMesh("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",r,"\x46\x69\x6C\x65\x4D\x65\x73\x68","\x32\x30\x33\x32\x39\x39\x37\x36","",v,Vector3.new(0,0,-v.X/8))elseif x=="\x52\x69\x6E\x67"then l=CreateMesh("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",r,"\x46\x69\x6C\x65\x4D\x65\x73\x68","\x35\x35\x39\x38\x33\x31\x38\x34\x34","",Vector3.new(v.X,v.X,0.1),Vector3.new())elseif x=="\x53\x6C\x61\x73\x68"then l=CreateMesh("\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68",r,"\x46\x69\x6C\x65\x4D\x6
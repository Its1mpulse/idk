c.Parent=a if(b)then for _,a in next,b do pcall(function()c[_]=a end)end end return c end function chatfunc(_,a)local c=string.gsub(_,"\xE2\x80\x8B","") local _=coroutine.wrap(function()local _=y:FindFirstChild("\x54\x61\x6C\x6B\x69\x6E\x67\x42\x69\x6C\x6C\x42\x6F\x61\x72\x64") if _ then _:Destroy()end local _=Instance.new("\x42\x69\x6C\x6C\x62\x6F\x61\x72\x64\x47\x75\x69",y) _.Size=UDim2.new(0,9999,2,0) _.StudsOffset=Vector3.new(0,5,0) _.Adornee=y _.Name="\x54\x61\x6C\x6B\x69\x6E\x67\x42\x69\x6C\x6C\x42\x6F\x61\x72\x64" local e=Instance.new("\x54\x65\x78\x74\x4C\x61\x62\x65\x6C",_) e.BackgroundTransparency=1 e.BorderSizePixel=0 e.Text="" e.Font="\x42\x61\x6E\x67\x65\x72\x73" e.TextScaled=true e.TextStrokeTransparency=0 coroutine.resume(coroutine.create(function()while not J do local a,b,_=Color3:ToHSV(f.NeonParts.Color) e.TextColor3=Color3.fromHSV(a,b,_/2) swait()end end)) e.TextStrokeColor3=b e.Size=UDim2.new(1,0,1,0) local d=Instance.new("\x54\x65\x78\x74\x4C\x61\x62\x65\x6C",_) d.BackgroundTransparency=1 d.BorderSizePixel=0 d.Text="" d.Font="\x42\x61\x6E\x67\x65\x72\x73" d.TextScaled=true d.TextStrokeTransparency=0 coroutine.resume(coroutine.create(function()while not J do d.TextColor3=f.NeonParts.Color swait()end end)) d.TextStrokeColor3=b d.Size=UDim2.new(1,0,1,0) coroutine.resume(coroutine.create(function()while not J and _~=nil do swait() if a then local _={"\x43\x6F\x64\x65","\x41\x72\x69\x61\x6C","\x43\x61\x72\x74\x6F\x6F\x6E","\x47\x6F\x74\x68\x61\x6D","\x48\x69\x67\x68\x77\x61\x79","\x4A\x6F\x73\x65\x66\x69\x6E\x53\x61\x6E\x73","\x4C\x65\x67\x61\x63\x79","\x4E\x75\x6E\x69\x74\x6F","\x4D\x65\x72\x72\x69\x77\x65\x61\x74\x68\x65\x72","\x52\x6F\x62\x6F\x74\x6F","\x53\x63\x69\x46\x69","\x53\x6F\x75\x72\x63\x65\x53\x61\x6E\x73","\x55\x62\x75\x6E\x74\x75"} local _=_[math.random(1,#_)] e.Font=_ d.Font=_ end e.Position=UDim2.new(0,math.random(-3,3),0,math.random(-3,3)) d.Position=UDim2.new(0,math.random(-3,3),0,math.random(-3,3))end end)) for _=1,string.len(c)do swait(2) e.Text=string.sub(c,1,_) d.Text=string.sub(c,1,_)end swait(120) for _=1,50 do swait() e.Position=e.Position-UDim2.new(0,math.random(-3,3),0,math.random(-3,3)) d.Position=e.Position-UDim2.new(0,math.random(-3,3),0,math.random(-3,3)) e.Rotation=e.Rotation+math.random(-2,2) d.Rotation=d.Rotation+math.random(-2,2) e.TextStrokeTransparency=_/50 e.TextTransparency=e.TextStrokeTransparency d.TextStrokeTransparency=e.TextStrokeTransparency d.TextTransparency=e.TextStrokeTransparency end _:Destroy()end) _()end function onChatted(_)chatfunc(_,true)end d.Chatted:connect(onChatted) local m=nil local o=nil local b=false local _=false local _=nil local _=nil local _=false local U="\x49\x64\x6C\x65" local _=1 local _=false local _=true local Z=(P.Velocity*Vector3.new(1,0,1)).Magnitude local i=0 Y.WalkSpeed=k local v=CFrame.new()*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(180)) local w=CFrame.new(0,1,0)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(180)) local x=CFrame.new(-.5,0,0)*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)) local F=CFrame.new(.5,0,0)*CFrame.Angles(math.rad(0),math.rad(-90),math.rad(0)) function QFCF(_)local _,_,_,h,c,b,f,i,g,e,d,a=_:components() local _=h+i+a if _>0 then local _=math.sqrt(1+_) local a=0.5/_ return(d-g)*a,(b-e)*a,(f-c)*a,_*0.5 else local _=0 if i>h then _=1 end if a>(_==0 and h or i)then _=2 end if _==0 then local _=math.sqrt(h-i-a+1) local a=0.5/_ return 0.5*_,(f+c)*a,(e+b)*a,(d-g)*a elseif _==1 then local _=math.sqrt(i-a-h+1) local a=0.5/_ return(c+f)*a,0.5*_,(d+g)*a,(b-e)*a elseif _==2 then local _=math.sqrt(a-h-i+1) local a=0.5/_ return(b+e)*a,(g+d)*a,0.5*_,(f-c)*a end end end function QTCF(_,a,b,h,k,l,d)local c,i,m=h+h,k+k,l+l local e,f,d=d*c,d*i,d*m local g=h*c local c=h*i local h=h*m local j=k*i local k=k*m local i=l*m return CFrame.new(_,a,b,1-(j+i),c-d,h+f,c+d,1-(g+i),k-e,h-f,k+e,1-(g+j))end function QS(e,a,c)local _=e[1]*a[1]+e[2]*a[2]+e[3]*a[3]+e[4]*a[4] local d,b if _>=0.0001 then if(1-_)>0.0001 then local a=math.acos(_) local _=1/math.sin(a) d=math.sin((1-c)*a)*_ b=math.sin(c*a)*_ else d=1-c b=c end else if(1+_)>0.0001 then local a=math.acos(-_) local _=1/math.sin(a) d=math.sin((c-1)*a)*_ b=math.sin(c*a)*_ else d=c-1 b=c end end return e[1]*d+a[1]*b,e[2]*d+a[2]*b,e[3]*d+a[3]*b,e[4]*d+a[4]*b end function Clerp(_,b,i)local f={QFCF(_)} local g={QFCF(b)} local e,d,a=_.x,_.y,_.z local b,c,_=b.x,b.y,b.z local h=1-i return QTCF(h*e+i*b,h*d+i*c,h*a+i*_,QS(f,g,i))end local W={} function W.nub(b,a,_)return b:Lerp(a,_)end function W.Clerp(_,a,b)return _:Lerp(a,b)end Qlerp=function(_,a,b)return _:Lerp(a,math.clamp(b,0,1))end ArtificialHB=Instance.new("\x42\x69\x6E\x64\x61\x62\x6C\x65\x45\x76\x65\x6E\x74",script) ArtificialHB.Name="\x48\x65\x61\x72\x74\x62\x65\x61\x74" script:WaitForChild("\x48\x65\x61\x72\x74\x62\x65\x61\x74") frame=1/60 tf=0 allowframeloss=false tossremainder=false lastframe=tick() script.Heartbeat:Fire() game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").Heartbeat:Connect(function(a,_)tf=tf+a if tf>=frame then if allowframeloss then script.Heartbeat:Fire() lastframe=tick()else for _=1,math.floor(tf/frame)do script.Heartbeat:Fire()end lastframe=tick()end if tossremainder then tf=0 else tf=tf-frame*math.floor(tf/frame)end end end) function swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end function Swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end SWAIT=Swait local _=Instance.new("\x53\x6F\x75\x6E\x64") local L=game.Players.LocalPlayer.Character local I=I(I(I(L,"\x53\x74\x61\x72\x73\x6C\x61\x79\x65\x72\x20\x52\x61\x69\x6C\x67\x75\x6E","\x41\x63\x63\x65\x73\x73\x6F\x72\x79"),"\x48\x61\x6E\x64\x6C\x65","\x42\x61\x73\x65\x50\x61\x72\x74"),"\x61\x74\x74\x31\x5F\x48\x61\x6E\x64\x6C\x65","\x41\x74\x74\x61\x63\x68\x6D\x65\x6E\x74") I.Parent=L["\x52\x69\x67\x68\x74\x20\x41\x72\x6D"] I.Position=Vector3.new(0,-1.5,-0.7) I.Orientation=Vector3.new(-0,90,-45) local _=1 function AttackGyro()local _=Instance.new("\x42\x6F\x64\x79\x47\x79\x72\x6F",P) _.D=25 _.P=20000 _.MaxTorque=Vector3.new(0,4000000,0) _.CFrame=CFrame.new(P.Position,g.Hit.p) coroutine.resume(coroutine.create(function()repeat if J then wait(math.huge)end swait() _.CFrame=CFrame.new(P.Position,g.Hit.p)until N==false _:Destroy()end))end coroutine.resume(coroutine.create(function()while not J do for _=0,1,(1/60)do swait() if not l then f.NeonParts.Color=Color3.fromHSV(_,1,1) if V.Value=="\x69\x4E\x53\x61\x4E\x69\x54\x59"then f.NeonParts.Color=Color3.fromHSV(math.random(0,19)/20,1,1)elseif V.Value=="\x45\x72\x72\x30\x72"then f.NeonParts.Color=Color3.fromRGB(0,math.clamp(Q.PlaybackLoudness-255,0,255),0)elseif V.Value=="\x47\x6C\x69\x74\x63\x68"then f.NeonParts.Color=Color3.fromHSV(_,1,math.clamp((Q.PlaybackLoudness/255)-.5,0,1))elseif V.Value=="\x41\x6C\x61\x73\x74\x6F\x72"then f.NeonParts.Color=Color3.new(Q.PlaybackLoudness/500,0,0)elseif V.Value=="\x6B\x41\x72\x6D\x41"then f.NeonParts.Color=Color3.new(1,1,1)elseif V.Value=="\x70\x65\x74\x66\x72\x6F\x67\x65"then f.NeonParts.Color=BrickColor.Random().Color end end end end end)) function attackone()N=true k=16 AttackGyro() for _=0,.5,.05 do swait() ha.C0=Clerp(ha.C0,ba*CFrame.new(0+.5*math.cos(la/50),0,0-.5*math.sin(la/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) ka.C0=Clerp(ka.C0,w*CFrame.new()*CFrame.Angles(math.rad(15),math.rad(0),math.rad(-30)),1/3) fa.C0=Clerp(fa.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(90),math.rad(0),math.rad(30))*x,1/3) ga.C0=Clerp(ga.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(30),math.rad(0),math.rad(0))*F,1/3) ja.C0=Clerp(ja.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(la/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(la/37)),math.rad(0),math.rad(0)),.7/3) ia.C0=Clerp(ia.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(la/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(la/41)),math.rad(0),math.rad(0)),.7/3)end for _=0,.5,.075 do swait() ha.C0=Clerp(ha.C0,ba*CFrame.new(0+.5*math.cos(la/50),0,0-.5*math.sin(la/50))*CFrame.Angles(math.rad(0),math.rad(0),math.rad(30)),.7/3) ka.C0=Clerp(ka.C0,w*CFrame.new()*CFrame.Angles(math.rad(10),math.rad(0),math.rad(-60)),1/3) fa.C0=Clerp(fa.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(160),math.rad(-20),math.rad(60))*x,1/3) ga.C0=Clerp(ga.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(40),math.rad(5),math.rad(5))*F,1/3) ja.C0=Clerp(ja.C0,CFrame.new(1,-.5,-.5)*CFrame.Angles(math.rad(-15+9*math.cos(la/74)),math.rad(80),math.rad(0))*CFrame.Angles(math.rad(0+5*math.cos(la/37)),math.rad(0),math.rad(0)),.7/3) ia.C0=Clerp(ia.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(-15-9*math.cos(la/54)),math.rad(-80),math.rad(0))*CFrame.Angles(math.rad(0-5*math.cos(la/41)),math.rad(0),math.rad(0)),.7/3)end k=50 N=false end local f=false iyflyspeed=1 function sFLY()if not h then f=false else local c={F=0,B=0,L=0,R=0,Q=0,E=0} local b={F=0,B=0,L=0,R=0,Q=0,E=0} local a=0 local function _()f=true m=Instance.new('\x42\x6F\x64\x79\x47\x79\x72\x6F',P) o=Instance.new('\x42\x6F\x64\x79\x56\x65\x6C\x6F\x63\x69\x74\x79',P) m.MaxTorque=Vector3.new(math.huge,math.huge,math.huge) m.P=1e4 m.CFrame=P.CFrame o.Velocity=Vector3.new(0,0,0) o.MaxForce=Vector3.new(math.huge,math.huge,math.huge) coroutine.resume(coroutine.create(function()repeat if J then wait(math.huge)end game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").RenderStepped:Wait() if not l then if c.L+c.R~=0 or c.F+c.B~=0 or c.Q+c.E~=0 then a=50 elseif not(c.L+c.R~=0 or c.F+c.B~=0 or c.Q+c.E~=0)and a~=0 then a=0 end if(c.L+c.R)~=0 or(c.F+c.B)~=0 or(c.Q+c.E)~=0 then o.Velocity=((workspace.CurrentCamera.CoordinateFrame.lookVector*(c.F+c.B))+((workspace.CurrentCamera.CoordinateFrame*CFrame.new(c.L+c.R,(c.F+c.B+c.Q+c.E)*0.2,0).Position)-workspace.CurrentCamera.CoordinateFrame.p))*a b={F=c.F,B=c.B,L=c.L,R=c.R}elseif(c.L+c.R)==0 and(c.F+c.B)==0 and(c.Q+c.E)==0 and a~=0 then o.Ve
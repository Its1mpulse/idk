end local d=NewInstance("\x42\x69\x6C\x6C\x62\x6F\x61\x72\x64\x47\x75\x69",B,{Name='\x43\x68\x61\x74\x47\x55\x49',Size=UDim2.new(0,100,0,40),StudsOffset=Vector3.new(0,3,0),Adornee=_}) local e=0 local _=0 for c=1,#b do e=e-16 _=_+32 delay(c/25,function()local _=NewInstance("\x54\x65\x78\x74\x4C\x61\x62\x65\x6C",d,{Text=b:sub(c,c),Position=UDim2.new(0,0,0,300),BackgroundTransparency=1,TextColor3=(typeof(a)=='\x73\x74\x72\x69\x6E\x67'and a:lower()=='\x72\x61\x69\x6E\x62\x6F\x77'and C3.HSV(Hue/360,1,1))or a,BorderSizePixel=0,Font=Enum.Font.SciFi,TextSize=40,TextStrokeTransparency=0,Size=UDim2.new(1,0,.5,0)}) coroutine.resume(coroutine.create(function()while true do swait() _.Rotation=math.random(-7,7) _.Position=_.Position+UDim2.new(0,math.random(-12,12)/7,0,math.random(-12,12)/7)end end)) coroutine.resume(coroutine.create(function()while true do swait() _.TextColor3=r end end)) e=e+32 if(_.Parent)then _:TweenPosition(UDim2.new(0,e,0,0),Enum.EasingDirection.InOut,Enum.EasingStyle.Quart,1)end end)end d.Size=UDim2.new(0,_,0,40) delay((#b/25)+3,function()for _,_ in next,d:children()do _:TweenPosition(UDim2.new(0,0,0,0),Enum.EasingDirection.InOut,Enum.EasingStyle.Quad,1)end wait(1.15) d:destroy()end)end)()end function onChatted(_)Chat(_,r)end c.Chatted:connect(onChatted) local k=nil local n=nil local b=false local _=false local _=nil local _=nil local _=false local U="\x49\x64\x6C\x65" local _=1 local _=false local _=true local Y=(P.Velocity*Vector3.new(1,0,1)).Magnitude local j=0 W.WalkSpeed=m local v=CFrame.new()*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(180)) local x=CFrame.new(0,1,0)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(180)) local z=CFrame.new(-.5,0,0)*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)) local D=CFrame.new(.5,0,0)*CFrame.Angles(math.rad(0),math.rad(-90),math.rad(0)) function QFCF(_)local _,_,_,i,g,f,b,h,d,c,e,a=_:components() local _=i+h+a if _>0 then local _=math.sqrt(1+_) local a=0.5/_ return(e-d)*a,(f-c)*a,(b-g)*a,_*0.5 else local _=0 if h>i then _=1 end if a>(_==0 and i or h)then _=2 end if _==0 then local _=math.sqrt(i-h-a+1) local a=0.5/_ return 0.5*_,(b+g)*a,(c+f)*a,(e-d)*a elseif _==1 then local _=math.sqrt(h-a-i+1) local a=0.5/_ return(g+b)*a,0.5*_,(e+d)*a,(f-c)*a elseif _==2 then local _=math.sqrt(a-i-h+1) local a=0.5/_ return(f+c)*a,(d+e)*a,0.5*_,(b-g)*a end end end function QTCF(a,_,b,j,m,l,d)local c,h,n=j+j,m+m,l+l local f,d,e=d*c,d*h,d*n local g=j*c local i=j*h local k=j*n local j=m*h local h=m*n local c=l*n return CFrame.new(a,_,b,1-(j+c),i-e,k+d,i+e,1-(g+c),h-f,k-d,h+f,1-(g+j))end function QS(a,e,c)local _=a[1]*e[1]+a[2]*e[2]+a[3]*e[3]+a[4]*e[4] local d,b if _>=0.0001 then if(1-_)>0.0001 then local a=math.acos(_) local _=1/math.sin(a) d=math.sin((1-c)*a)*_ b=math.sin(c*a)*_ else d=1-c b=c end else if(1+_)>0.0001 then local a=math.acos(-_) local _=1/math.sin(a) d=math.sin((c-1)*a)*_ b=math.sin(c*a)*_ else d=c-1 b=c end end return a[1]*d+e[1]*b,a[2]*d+e[2]*b,a[3]*d+e[3]*b,a[4]*d+e[4]*b end function Clerp(a,_,i)local g={QFCF(a)} local f={QFCF(_)} local d,e,c=a.x,a.y,a.z local b,_,a=_.x,_.y,_.z local h=1-i return QTCF(h*d+i*b,h*e+i*_,h*c+i*a,QS(g,f,i))end local X={} function X.nub(_,a,b)return _:Lerp(a,b)end function X.Clerp(a,b,_)return a:Lerp(b,_)end ArtificialHB=Instance.new("\x42\x69\x6E\x64\x61\x62\x6C\x65\x45\x76\x65\x6E\x74",script) ArtificialHB.Name="\x48\x65\x61\x72\x74\x62\x65\x61\x74" script:WaitForChild("\x48\x65\x61\x72\x74\x62\x65\x61\x74") frame=1/60 tf=0 allowframeloss=false tossremainder=false lastframe=tick() script.Heartbeat:Fire() game:GetService("\x52\x75\x6E\x53\x65\x72\x76\x69\x63\x65").Heartbeat:Connect(function(a,_)tf=tf+a if tf>=frame then if allowframeloss then script.Heartbeat:Fire() lastframe=tick()else for _=1,math.floor(tf/frame)do script.Heartbeat:Fire()end lastframe=tick()end if tossremainder then tf=0 else tf=tf-frame*math.floor(tf/frame)end end end) function swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end function Swait(_)if _==0 or _==nil then ArtificialHB.Event:Wait()else for _=0,_ do ArtificialHB.Event:Wait()end end end SWAIT=Swait function CreateConnection(_,b,e,d,a,c)if K then wait(math.huge)end local _=Instance.new(_) _.Part0=e _.Part1=d _.C0=a _.C1=c _.Parent=b return _ end function CreateMesh(d,c,a,f,e,b,_)local g=Instance.new(d) if d=="\x53\x70\x65\x63\x69\x61\x6C\x4D\x65\x73\x68"then g.MeshType=a if f~="\x6E\x69\x6C"and f~=""and tonumber(f)~=nil then g.MeshId="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F"..f elseif f~="\x6E\x69\x6C"and f~=""then g.MeshId=f end if e~="\x6E\x69\x6C"and e~=""then g.TextureId="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F"..e end end g.Offset=_ or Vector3.new() g.Scale=b g.Parent=c return g end function CreatePart(f,d,e,g,c,b,_,_,a)local h=Instance.new("\x50\x61\x72\x74") h.Massless=true h.formFactor=f h.Reflectance=g h.Transparency=c h.CanCollide=false h.Locked=true h.Anchored=true if a==false then h.Anchored=false end h.Color=b h.Name=t() h.Size=_ h.Position=B.Position h.Material=e h:BreakJoints() h.Parent=d return h end local e=Instance.new("\x53\x6F\x75\x6E\x64") function CreateSound(_,a,c,b,d)local f=nil coroutine.resume(coroutine.create(function()f=e:Clone() f.Parent=a f.Volume=c f.Pitch=b f.SoundId="\x72\x62\x78\x61\x73\x73\x65\x74\x69\x64\x3A\x2F\x2F".._ f:play() if d==true then f.Looped=true else coroutine.resume(coroutine.create(function()pcall(function()repeat if K then wait(math.huge)end Swait()until f.Playing==false f:Destroy()end)end))end end)) return f end local function a(_,a)local b=Instance.new("\x57\x65\x6C\x64") b.Part0=_ b.Part1=a b.C1=a.CFrame:inverse()*_.CFrame b.Parent=a return b end function rayCast(a,_,b,c)return workspace:FindPartOnRay(Ray.new(a,_.unit*(b or 999.999)),c)end function CreateFork(f,h,g,d,e,c,b,a,_)local i=IT("\x50\x61\x72\x74") i.formFactor=f i.Reflectance=d i.Transparency=e i.CanCollide=false i.Locked=true i.Anchored=true if _==false then i.Anchored=fal